@model IEnumerable<string>

@foreach (var link in Model)
{
    @Html.RouteLink(link, new
    {
        controller = "Dish",
        action = "List",
        category = link,
        page = 1
    }, new
    {
        @class = "categories-btn"
            + (link == ViewBag.SelectedCategory ? " btn-primary" : "")
    })
}