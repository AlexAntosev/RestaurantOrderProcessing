@using RestaurantOrderProcessing.WebUI.Models
@using RestaurantOrderProcessing.WebUI.HtmlHelpers
@model DishesListViewModel

@{
    ViewBag.Title = "Dishes";
}

<main class="main">    
    <div class="dishescol">
        @foreach (var dish in @Model.Dishes)
        {
            @Html.Partial("DishSummary", dish);
        }
    </div>
</main>

